@{
    ViewData["Title"] = "Home Page";
}
<main class="container main-content mx-auto">
    <form class="row g-3">
        <div class="input-group mb-3">
            <input type="file" class="form-control" id="inputGroupFile02">
            <label class="input-group-text" for="inputGroupFile02">Photo</label>
        </div>
        <div class="col-md-4">
            <label for="validationDefault01" class="form-label">First name</label>
            <input type="text" class="form-control" id="validationDefault01" required>
        </div>
        <div class="col-md-4">
            <label for="validationDefault02" class="form-label">Last name</label>
            <input type="text" class="form-control" id="validationDefault02" required>
        </div>
        <div class="col-md-6">
            <label for="validationDefault03" class="form-label">Email</label>
            <input type="text" class="form-control" id="validationDefault03" required>
        </div>
        <div class="col-md-6">
            <label for="validationDefault04" class="form-label">Address</label>
            <input type="text" class="form-control" id="validationDefault04" required>
        </div>
        <div class="col-md-6">
            <label for="validationDefault05" class="form-label">City</label>
            <select class="form-select" id="validationDefault05" required>
                <option selected disabled value="">Choose...</option>
                <option>...</option>
            </select>
        </div>
        <div class="col-md-3">
            <label for="validationDefault06" class="form-label">County</label>
            <select class="form-select" id="validationDefault06" required>
                <option selected disabled value="">Choose...</option>
                <option>...</option>
            </select>
        </div>
        <div class="col-md-3">
            <label for="validationDefault07" class="form-label">Post Code</label>
            <input type="text" class="form-control" id="validationDefault07" required>
        </div>
        <div class="col-12">
            <div class="form-check">
                <input class="form-check-input" type="checkbox" value="" id="invalidCheck2" required>
                <label class="form-check-label" for="invalidCheck2">
                    Agree to terms and conditions
                </label>
            </div>
        </div>
        <div class="col-12">
            <button class="btn btn-primary" type="submit">Submit form</button>
        </div>
    </form>
</main>